{
  "name": "mosh2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "del-cli public/js/app && webpack --config webpack.config.dev.js --progress --profile --watch",
    "build:prod": "del-cli public/js/app && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'public/js/app/**/*.js' 'public/js/app/**/*.js.map' '!public/js/app/bundle.js' '!public/js/app/*.chunk.js' 'assets/app/**/*.ngfactory.ts' 'assets/app/**/*.shim.ts'"
  },
  "dependencies": {
    "@angular/common": "^5.0.5",
    "@angular/compiler": "^5.0.5",
    "@angular/core": "^5.0.5",
    "@angular/forms": "^5.1.1",
    "@angular/http": "^5.0.5",
    "@angular/platform-browser": "^5.0.5",
    "@angular/platform-browser-dynamic": "^5.0.5",
    "@angular/router": "^5.0.5",
    "@angular/upgrade": "^5.0.5",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.8",
    "angular2-social-login": "^3.1.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.5.1",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "~4.15.5",
    "hbs": "~4.0.1",
    "install": "^0.10.2",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^4.13.7",
    "mongoose-unique-validator": "^1.0.6",
    "morgan": "~1.9.0",
    "npm": "^5.6.0",
    "rxjs": "^5.5.4",
    "serve-favicon": "~2.4.5",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.43",
    "@types/node": "^8.0.55",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.1",
    "del-cli": "^1.1.0",
    "html-loader": "^0.5.1",
    "raw-loader": "^0.5.1",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  }
